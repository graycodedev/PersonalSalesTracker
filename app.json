{
  "expo": {
    "name": "FINMAX Sales App",
    "slug": "finmax-sales-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.graycode.finmaxsales",

      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to let user put a photo in his profile page."
      },
      "buildNumber": "9"
    },
    "android": {
      "package": "com.graycode.finmaxsales",
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "LOCATION","ACCESS_COARSE_LOCATION","ACCESS_FINE_LOCATION"],
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "9fb69b05-27f7-4c82-8712-a60c116c925e"
      }
    },
    "sdkVersion": "49.0.0",
    "owner": "graycodedev"
  }
}
